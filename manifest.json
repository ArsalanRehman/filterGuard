{
    "manifest_version": 3,
    "name": "NSFW Word Censor",
    "version": "1.0",
    "description": "Censors offensive or NSFW words from webpages.",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  